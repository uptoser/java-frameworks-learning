<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uptoser.ssm.spring.c5.transactional.mapper.RoleMapper">
    <insert id="insertRole" parameterType="com.uptoser.ssm.spring.c4.db.pojo.Role">
        insert into t_role (role_name, note) values(#{roleName}, #{note})
    </insert>
</mapper>